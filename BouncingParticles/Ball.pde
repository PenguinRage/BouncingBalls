
/* Ball Class: contains properties of the ball
 *
 *
 */

class Ball {
  int x;
  int y;
  int z;

  // Setters
  void setX(int x) { this.x = x; }
  void setY(int y) { this.y = y; }
  void setZ(int z) { this.z = z; }

  // Getters
  int getX() { return x; }
  int getY() { return y; }
  int getZ() { return z; }
}
